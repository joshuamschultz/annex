<p id="notice"><%= notice %></p>

<h1>Imports</h1>

<table id="table">
  <thead>
    <tr>
      <th>Assigned number</th>
      <th>Pedimento number</th>
      <th>Import type</th>
      <th>Total</th>
      <th>Iva</th>
      <th>Notes</th>
      <th>Import date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @imports.each do |import| %>
      <tr>
        <td><%= import.assigned_number %></td>
        <td><%= import.pedimento_number %></td>
        <td><%= import.import_type_id %></td>
        <td><%= import.total %></td>
        <td><%= import.iva %></td>
        <td><%= import.notes %></td>
        <td><%= import.import_date %></td>
        <td><%= link_to 'Show', import %></td>
        <td><%= link_to 'Edit', edit_import_path(import) %></td>
        <td><%= link_to 'Destroy', import, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Import', new_import_path %>



<script>
  $(document).ready( function () {
    $('#table').DataTable();
  } );
</script>