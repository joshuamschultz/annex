.widget-body
  %table{id: "usage_block", class: "dynamicTable table table-striped table-bordered table-condensed",data: { source: reports_usage_path(format: :json) }}
    %thead
      %tr
        %th Part Number
        %th= @keys[0]
        %th= @keys[1]
        %th= @keys[2]
        %th= @keys[3]
        %th= @keys[4]
        %th= @keys[5]
        %th= @keys[6]
        %th= @keys[7]
        %th= @keys[8]
        %th= @keys[9]
        %th= @keys[10]
        %th= @keys[11]
    %tbody
- console

:javascript
  $('#usage_block').DataTable({
    ajax: $('#usage_block').data()['source'], 
    columns: [
        {data: 'Part Number'},
        {data: 0},
        {data: 1},
        {data: 2},
        {data: 3},
        {data: 4},
        {data: 5},
        {data: 6},
        {data: 7},
        {data: 8},
        {data: 9},
        {data: 10},
        {data: 11}
    ]
  });
